---
import Layout from '../../../layouts/Layout.astro';
import {getStaticPathsForLanguages, useTranslations} from '../../../i18n/utils';
 
export async function getStaticPaths() {
  return getStaticPathsForLanguages("product");
}

const t = useTranslations(Astro.url);
---

<Layout title="Welcome to Astro.">
	<main>
		<h1>Product! {t('home')}</h1>
	</main>
</Layout>

